{
  "presets": [
    [
      "@babel/preset-react",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 5 versions",
            "ie >= 9"
          ]
        },
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-optional-chaining",
    "lodash",
    "react-hot-loader/babel",
    ["@babel/plugin-proposal-decorators", {"legacy": true}],
    ["@babel/plugin-proposal-class-properties",{"loose": false}]
  ]
}